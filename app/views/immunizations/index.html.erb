<div class="ft-sz-25">
  <% @immunizations.each do |immunization| %>
    <p>Pharmacy Name: <%= immunization.name %></p><br>
    <p>Patient Name: <%= immunization.date %></p><br>
    <p><%= immunization.updated_at.strftime("Last updated %m/%d/%Y, at %l:%M %p") %></p><br>
    <p><%= link_to 'Go To This Immunization' , immunization_path(immunization) %>
    
      
      <br>   
    
  <% end %>
</div>

 

